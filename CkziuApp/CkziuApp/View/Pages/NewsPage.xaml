<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CkziuApp.View.Pages.NewsPage" Title="Aktualności">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout>
                <WebView x:Name="webView" Source="http://csharp-dev.pl/Elektryk/news.php" WidthRequest="1000" HeightRequest="1000"/>
            </StackLayout>
            <StackLayout Orientation="Horizontal" Padding="10,10">
                <Button Text="Wstecz" HorizontalOptions="StartAndExpand" Clicked="backClicked" />
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>